{
  "/api/*": {
    "target": "http://localhost:3000",
    "secure": false,
    "pathRewrite": {"^/api" : "http://localhost:3000/api"},
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/system/*": {
    "target": "http://localhost:3000",
    "secure": false,
    "pathRewrite": {"^/system" : "http://localhost:3000/system"},
    "changeOrigin": true,
    "logLevel": "debug"
  }
}